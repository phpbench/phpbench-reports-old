{% extends "layout.html.twig" %}
{% block content %}
<table class="ui table">
    <thead>
        <tr>
            <th>Subject</th>
            <th>Count</th>
        </tr>
    </thead>
    <tbody>
{% for subject in subjects %}
<tr>
    <td><a href="{{ path('subject', { 'class': subject.benchmark, 'name': subject.name }) }}">{{ subject.name }}</a></td>
    <td>{{ subject.count }}</a></td>
</tr>
{% endfor %}
    </tbody>
</table>
{% endblock %}
